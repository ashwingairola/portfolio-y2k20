{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"module": "es2020",
		"moduleResolution": "node",
		"importHelpers": true,
		"target": "es2015",
		"typeRoots": ["node_modules/@types"],
		"lib": ["es2018", "dom"],
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"paths": {
			"@ag-apis": ["src/app/apis/index.ts"],
			"@ag-env": ["src/environments/environment"],
			"@ag-models": ["src/app/models/index.ts"],
			"@ag-modules": ["src/app/modules/index.ts"],
			"@ag-services": ["src/app/services/index.ts"],
			"@ag-tailwind-config": ["tailwind.config.js"]
		}
	},
	"angularCompilerOptions": {
		"fullTemplateTypeCheck": true,
		"strictInjectionParameters": true
	}
}
